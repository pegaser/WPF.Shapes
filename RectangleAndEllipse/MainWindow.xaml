<Window x:Class="RectangleAndEllipse.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:RectangleAndEllipse"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <StackPanel>
        <Ellipse Fill="Aqua" Stroke="DarkGoldenrod" Height="100" Width="100" 
                 HorizontalAlignment="Left" Margin="5"></Ellipse>
        <Rectangle Fill="Aqua" Stroke="DarkGoldenrod" Height="100" Width="100" 
                 HorizontalAlignment="Left" Margin="5" RadiusX="5" RadiusY="50"></Rectangle>
    </StackPanel>
</Window>
